<div class="main">
  <div class="container">
    ユーザ登録
    <% if @book.errors.full_messages %>
      <% @book.errors.full_messages.each do |message| %>
        <%= message %>
      <% end %>
    <% end %>
    <%= form_tag("/users/signup",{multipart:true}) do %>
      <p>タイトル</p>
      <input name="name" value="<%= @name %>">
      <p>内容</p>
      <input name="group" value="<%= @content %>">
      <p>画像</p>
      <input name="image" type="file">
      <input type="submit" value="新規登録">
    <% end %>
  </div>
</div>